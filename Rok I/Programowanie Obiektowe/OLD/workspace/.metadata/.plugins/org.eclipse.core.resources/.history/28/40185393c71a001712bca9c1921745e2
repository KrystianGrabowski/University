
public class Main {

	/**
	 * @param args
	 */
	public static void main(String[] args) {

        Slownik<String, Integer> slow = new Slownik<String, Integer>();
        slow.Dodaj("ala", 3);
        slow.Dodaj("kot", 123);
        slow.Dodaj("krzys", 1211231231);
        slow.Dodaj("stanislaw", 231);
        System.out.printf("{0}, {1}", "ala", slow.Szukaj("ala"));
        slow.Usun("ala");
        slow.Dodaj("krzys", 1211231231);
        System.out.printf("{0}, {1}", "krzys", slow.Szukaj("krzys"));
        System.out.printf("{0}, {1}","ala -",slow.Szukaj("ala"));
        System.out.println(slow.Szukaj("stasko"));

	}

}
