
public class Klient extends Czlowiek {
	DataK DX;
	DataK DY;
	
	public Klient(String i, String n, String x, String y, int w){
		imie = i;
		nazwisko = n;
		wiek = w;
		String T[] = x.split("-");
		DX = new DataK(Integer.valueOf(T[0]),Integer.valueOf(T[1]),Integer.valueOf(T[2]));
		String Ta[] = y.split("-");
		DY = new DataK(Integer.valueOf(Ta[0]),Integer.valueOf(Ta[1]),Integer.valueOf(Ta[2]));
	}
	
	public DataK KarnetP(){
		return DX;
	}
	
	public DataK KarnetK(){
		return DY;
	}
	
}
